@article{hosking_accumulation_2017,
	title = {Accumulation in coastal West Antarctic ice core records and the role of cyclone activity},
	volume = {44},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/2017GL074722/abstract},
	doi = {10.1002/2017GL074722},
	abstract = {Cyclones are an important component of Antarctic climate variability, yet quantifying their impact on the polar environment is challenging. We assess how cyclones which pass through the Bellingshausen Sea affect accumulation over Ellsworth Land, West Antarctica, where we have two ice core records. We use self-organizing maps ({SOMs}), an unsupervised machine learning technique, to group cyclones into nine {SOM} nodes differing by their trajectories (1980–2015). The annual frequency of cyclones associated with the first {SOM} node ({SOM}1, which generally originate from lower latitudes over the South Pacific Ocean) is significantly (p {\textless} 0.001) correlated with annual accumulation, with the highest seasonal correlations (p {\textless} 0.001) found during autumn. While significant (p {\textless} 0.01) increases in vertically integrated water vapor over the South Pacific Ocean coincide with this same group of cyclones, we find no indication that this has led to an increase in moisture advection into, nor accumulation over, Ellsworth Land over this short time period.},
	pages = {2017GL074722},
	number = {17},
	journaltitle = {Geophysical Research Letters},
	shortjournal = {Geophys. Res. Lett.},
	author = {Hosking, J. Scott and Fogt, Ryan and Thomas, Elizabeth R. and Moosavi, Vahid and Phillips, Tony and Coggins, Jack and Reusch, David},
	urldate = {2018-02-06},
	date = {2017-09-16},
	langid = {english},
	keywords = {ice cores, accumulation, 0724 Ice cores, 0764 Energy balance, 3349 Polar meteorology, cyclones, {SOMs}, West Antarctic},
	file = {Hosking et al_2017_Accumulation in coastal West Antarctic ice core records and the role of cyclone.pdf:/home/durbank/Documents/zotero-attachments/Hosking et al_2017_Accumulation in coastal West Antarctic ice core records and the role of cyclone.pdf:application/pdf;Snapshot:C\:\\Users\\Durban.Keeler\\Zotero\\storage\\P2J6FBR9\\abstract.html:text/html},
