	langid = {english},
	note = {Publisher: Cambridge University Press},
	keywords = {snow/ice surface processes, surface mass budget, accumulation, Antarctic glaciology, ice/atmosphere interactions},
	file = {Lenaerts et al_2018_Climate and surface mass balance of coastal West Antarctica resolved by.pdf:/home/durbank/Documents/zotero-attachments/Lenaerts et al_2018_Climate and surface mass balance of coastal West Antarctica resolved by.pdf:application/pdf},
