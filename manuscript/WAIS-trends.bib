	shortjournal = {Rev. Geophys.},
	author = {Eisen, Olaf and Frezzotti, Massimo and Genthon, Christophe and Isaksson, Elisabeth and Magand, Olivier and van den Broeke, Michiel R. and Dixon, Daniel A. and Ekaykin, Alexey and Holmlund, Per and Kameda, Takao and Karlof, Lars and Kaspari, Susan and Lipenkov, Vladimir Y. and Oerter, Hans and Takahashi, Shuhei and Vaughan, David G.},
	date = {2008-04-11},
	note = {{WOS}:000255079700001},
	keywords = {dronning-maud-land, shallow firn cores, automatic weather stations, electromagnetic-wave speed, greenland ice-sheet, lambert glacier basin, northern victoria-land, past 2 kyr, stable-isotope   measurements, surface mass-balance},
	file = {Eisen et al_2008_Ground-based measurements of spatial and temporal variability of snow.pdf:/home/durbank/Documents/zotero-attachments/Eisen et al_2008_Ground-based measurements of spatial and temporal variability of snow.pdf:application/pdf},
