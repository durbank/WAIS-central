	file = {Kuipers Munneke et al_2017_Observationally constrained surface mass balance of Larsen C ice shelf,.pdf:/home/durbank/Documents/zotero-attachments/Kuipers Munneke et al_2017_Observationally constrained surface mass balance of Larsen C ice shelf,.pdf:application/pdf},
